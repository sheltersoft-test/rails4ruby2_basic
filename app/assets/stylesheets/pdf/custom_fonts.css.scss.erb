@font-face {
  font-family: 'Helvetica Neue';
  src: url('<%= asset_path("HelveticaNeue-Light.eot") %>');
  src: url('<%= asset_path("HelveticaNeue-Light.eot?#iefix") %>') format('embedded-opentype'),
       url('<%= asset_path("HelveticaNeue-Light.woff2") %>') format('woff2'),
       url('<%= asset_path("HelveticaNeue-Light.woff") %>') format('woff'),
       url('<%= asset_path("HelveticaNeue-Light.ttf") %>')) format('truetype'),
       url('<%= asset_path("HelveticaNeue-Light.svg#HelveticaNeue-Light") %>') format('svg');
  font-weight: 300;
  font-style: normal;
}

